# Esempio di utilizzo
borsa = Borsa()
ordine1 = Ordine("AAPL", 150, 10, "acquisto")
ordine2 = Ordine("AAPL", 155, 5, "acquisto")
borsa.esegui_ordine(ordine1)
borsa.esegui_ordine(ordine2)
